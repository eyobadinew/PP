							"});\r",
							"pm.test(\"Verify active value is true\", function () {\r",
							"    var jsonData = pm.response.json();\r",
							"    pm.expect(jsonData.active).to.eql(true);\r",
							"});\r",
							"pm.test(\"Verify Practitioner Name is Eyob Bizuneh\", function () {\r",
